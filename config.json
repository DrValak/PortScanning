{
  "scan_settings": {
    "default_timeout": 1,
    "max_threads": 100,
    "default_start_port": 1,
    "default_end_port": 100
  },
  "targets": {
    "localhost": "127.0.0.1",
    "example": "scanme.nmap.org"
  },
  "common_ports": [20, 21, 22, 23, 25, 53, 80, 110, 143, 443, 445, 3306, 3389, 5432, 8080, 8443],
  "logging": {
    "enabled": true,
    "log_file": "port_scanner.log",
    "log_level": "INFO"
  },
  "output": {
    "save_results": true,
    "output_format": "json",
    "output_directory": "scan_results"
  }
}
